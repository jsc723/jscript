proc initialArray(var length)
{
    arr array;
    var i 0;
    while(i<length)
    {
        append array eval(int(30*cos(i)));
        set i i+1;
    };
    return obj array;
};
proc sortArray(arr array,var length)
{
    var i 0,j 0,t;
    while(i<length-1)
    {
        set j 0;
        while(j<length-1)
        {
            if(array[j]>array[j+1])
            {
                set t array[j];
                set array[j] array[j+1];
                set array[j+1] t;
            };
            set j j+1;
        };
        set i i+1;
    };
    return obj array;
};

var n;
arr a;
set arr a initialArray(10);
print("array:",a,"\n");
set arr a sortArray(a,10);
print("sorted array:",a,"\n");
pause "Press any key to continue...";

