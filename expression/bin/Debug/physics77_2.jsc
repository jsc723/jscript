#err(f5);
proc fact(var n)
{
    if(n==0 || n==1)
    { return n; }
    else
    { return n*fact(n-1); };
};
proc poisson(var ru,var n)
{
    return ru^n*exp(-1*ru)/fact(n);
};
var i 1,s 0;
while(i<=19)
{
    set s s+poisson(7.293,i);
    set i i+1;
};
print("poisson 1-19 = ",eval(s));
